      </page>
      <page
            category="org.eclipse.papyrus.preferences.properties.generalcategory"
            class="org.eclipse.papyrus.preferences.pages.internal.PapyrusRulersAndGridPreferencePage"
            id="org.eclipse.papyrus.preferences.properties.rulersandgrid"
            name="Rulers And Grid">
