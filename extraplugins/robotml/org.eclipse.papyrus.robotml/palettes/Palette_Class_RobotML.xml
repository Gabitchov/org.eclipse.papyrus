<?xml version="1.0" encoding="UTF-8"?>
<paletteDefinition>
	<content>
		<drawer iconpath="/icons/drawer.gif" id="org.eclipse.papyrus.robotml.diagram.clazz"
			name="RobotML">
			<aspectTool description="Create a new Robotic System" id="clazz.tool.system"
