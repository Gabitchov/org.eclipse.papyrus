<plugin>
   <extension
         point="org.eclipse.papyrus.emf.facet.util.emf.core.catalogmanager">
      <catalog
            class="org.eclipse.papyrus.emf.facet.custom.core.internal.CustomizationCatalogManager">
      </catalog>
   </extension>
   
