			small_icon="platform:/plugin/org.eclipse.papyrus.sysml/icons/blocks/Block.gif">
		</entry>
			
		<!-- internalblock.tool.blockpropertycomposite -->
		<entry
			defineOnly="true"
			description="Create a BlockPropertyComposite"
